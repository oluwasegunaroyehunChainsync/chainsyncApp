# ============================================
# ChainSync Frontend - Production Configuration
# ============================================
# IMPORTANT: Update these values before deployment!

# API Configuration
VITE_API_URL=https://api.chainsync.org

# Application
VITE_APP_NAME=ChainSync
VITE_ENABLE_MOCK_DATA=false

# WalletConnect Project ID (get from https://cloud.walletconnect.com)
VITE_WALLETCONNECT_PROJECT_ID=YOUR_WALLETCONNECT_PROJECT_ID

# Analytics (optional)
VITE_GA_TRACKING_ID=
VITE_SENTRY_DSN=

# Feature Flags
VITE_ENABLE_TESTNET=false
VITE_ENABLE_GOVERNANCE=true
VITE_ENABLE_STAKING=true
